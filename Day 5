
#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    printf("%s",(n>=50)?"Pass":"Fail");
    return 0;
}
#include <stdio.h>
#define PI 3.14

int main() {
    float l, b, side, base, height, radius;
    
    // Rectangle
    printf("Enter length and breadth of rectangle: ");
    scanf("%f %f", &l, &b);
    printf("Area of Rectangle = %.2f\n", l * b);

    // Square
    printf("Enter side of square: ");
    scanf("%f", &side);
    printf("Area of Square = %.2f\n", side * side);

    // Triangle
    printf("Enter base and height of triangle: ");
    scanf("%f %f", &base, &height);
    printf("Area of Triangle = %.2f\n", 0.5 * base * height);

    // Circle
    printf("Enter radius of circle: ");
    scanf("%f", &radius);
    printf("Area of Circle = %.2f\n", PI * radius * radius);

    return 0;
}
3)#include <stdio.h>
#include <math.h>

int main() {
    float x1, y1, x2, y2, distance;

    printf("Enter x1 and y1: ");
    scanf("%f %f", &x1, &y1);

    printf("Enter x2 and y2: ");
    scanf("%f %f", &x2, &y2);

    distance = sqrt((x2 - x1)*(x2 - x1) + (y2 - y1)*(y2 - y1));

    printf("Distance between two points = %.2f\n", distance);

    return 0;
}
4)#include <stdio.h>

int main() {
    float p, t, r, si;

    printf("Enter Principal, Time (in years) and Rate: ");
    scanf("%f %f %f", &p, &t, &r);

    si = (p * t * r) / 100;

    printf("Simple Interest = %.2f\n", si);

    return 0;
}
